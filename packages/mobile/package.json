{
  "name": "note-companion",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "start": "expo start",
    "android": "expo run:android",
    "dev": "expo run:ios",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "release:ios": "eas build --platform ios",
    "build:android": "eas build --platform android --local",
    "build:ios": "eas build --platform ios --local --profile production --non-interactive",
    "build:ios:remote": "eas build --platform ios --profile production --non-interactive",
    "submit:ios": "eas submit --platform ios --latest --non-interactive",
    "build-and-submit:ios": "pnpm run build:ios && eas submit --platform ios --latest --non-interactive"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "7.26.9",
    "@clerk/clerk-expo": "^0.20.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "15.0.3",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "expo": "~54.0.30",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "^7.0.8",
    "expo-asset": "~12.0.12",
    "expo-blur": "15.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-font": "14.0.10",
    "expo-haptics": "^15.0.8",
    "expo-image-manipulator": "^14.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-localization": "^17.0.8",
    "expo-media-library": "~18.2.1",
    "expo-notifications": "^0.32.15",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-share-intent": "^5.1.1",
    "expo-sharing": "^14.0.8",
    "expo-splash-screen": "31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "1.0.8",
    "expo-system-ui": "^6.0.9",
    "expo-web-browser": "^15.0.10",
    "nativewind": "^4.1.23",
    "patch-package": "^8.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-blob-util": "^0.21.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "^4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "0.21.2",
    "react-native-webview": "^13.15.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@expo/metro-runtime": "^6.1.2",
    "@types/jest": "29.5.14",
    "@types/react": "^19.1.17",
    "@types/react-dom": "^19.1.11",
    "@types/react-test-renderer": "18.0.7",
    "babel-plugin-module-resolver": "^5.0.2",
